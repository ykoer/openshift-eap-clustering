          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Calculate Total" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">
double price = Double.parseDouble(vars.get(&apos;PRICE&apos;))
double quantity = Double.parseDouble(vars.get(&apos;QUANTITY&apos;))


def total = vars.getObject(&quot;TOTAL&quot;);
if (total==null) {
	total = 0;
}


total += (price*quantity)
vars.putObject(&quot;TOTAL&quot;, total.round(2))

log.info(price + &quot;:&quot; + total)



</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223Sampler>
